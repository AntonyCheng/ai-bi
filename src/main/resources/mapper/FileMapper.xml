<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.sharehome.springbootinittemplate.mapper.FileMapper">

    <resultMap id="BaseResultMap" type="top.sharehome.springbootinittemplate.model.entity.File">
        <id property="id" column="file_id" jdbcType="BIGINT"/>
        <result property="name" column="file_name" jdbcType="VARCHAR"/>
        <result property="url" column="file_url" jdbcType="VARCHAR"/>
        <result property="suffix" column="file_suffix" jdbcType="VARCHAR"/>
        <result property="userId" column="file_user_id" jdbcType="BIGINT"/>
        <result property="chartId" column="file_chart_id" jdbcType="BIGINT"/>
        <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
        <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
        <result property="isDeleted" column="is_deleted" jdbcType="TINYINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        file_id,file_name,file_url,
        file_suffix,file_user_id,file_chart_id,
        create_time,update_time,is_deleted
    </sql>
</mapper>
